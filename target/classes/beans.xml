<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="movie" class="com.stackroute.domain.Movie" autowire="byType">
        <property name="movieId" value="25"></property>
        <property name="movieName" value="Maharshi"></property>
    </bean>

    <bean name="actor" class="com.stackroute.domain.Actor">

        <property name="name" value="Mahesh"></property>
        <property name="gender" value="Male"></property>
        <property name="age" value="40"></property>

    </bean>

</beans>